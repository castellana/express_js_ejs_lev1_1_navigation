<%- include('./partials/header')%>

<h1>This is the gallery page</h1>
<section>
    <div class="my-gallery">
    <% gallery.forEach(element => { %>
        <figure>
            <img src="<%= element.download_url %>" alt="picture">
            <figcaption>
                <%=element.author %>
            </figcaption>

        </figure>
        
    <% }) %>
</div>
</section>
